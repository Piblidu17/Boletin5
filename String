typedef struct {
 int sign;
 int tam;
 int *val;
} bignum;
bignum str2bignum(char *str){//converte unha cadea a bignum
    bignum bn;
    int i,j;
    if(str[0]=='-'){
        bn.sign = -1;
        }
    else(str[0]=='+'){
        bn.sign=1
    }
    bn.tam=strlen(str)-1;
    bn.val=(int*)malloc(bn.tam*(sizeof(int)));
    j=0;
    for(i=(bn.tam); i>0; i--){
        bn.val[j]=(str[i]-48);
        j++;
    }
    return bn;
}
